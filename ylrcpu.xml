<?xml version="1.0" encoding="UTF-8"?>
<nf:module xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
	<nf:name>ylrcpu</nf:name>
	<nf:prefix>ylrcpu</nf:prefix>
	<nf:location>udp</nf:location>
	<nf:description>Registers for CPU</nf:description>
	<nf:blocksize>64</nf:blocksize>
	<nf:registers>
		<nf:register>
		  <nf:name>command_ylrcpu</nf:name>
			<nf:description>Commands for write or read</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>address_ylrcpu</nf:name>
			<nf:description>Address to write into IMem/DMem or read from DMem</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>imem</nf:name>
			<nf:description>Instruction to be stored in IMem BRAM</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>upper_dmem</nf:name>
			<nf:description>Upper 32 bits of DMem</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>lower_dmem</nf:name>
			<nf:description>Lower 32 bits of DMem</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
        <nf:register>
		  <nf:name>upper_dmem_read</nf:name>
			<nf:description>Reads Upper 32 bits of DMem</nf:description>
			<nf:type>generic_hardware32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>lower_dmem_read</nf:name>
			<nf:description>Reads Lower 32 bits of DMem</nf:description>
			<nf:type>generic_hardware32</nf:type>
		</nf:register>
	</nf:registers>
</nf:module>

